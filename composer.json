{
    "name": "chucknorris/api",
    "description": "chucknorris.io is a free JSON API for hand curated Chuck Norris facts.",
    "homepage": "https://api.chucknorris.io",
    "keywords": ["Chuck", "Norris", "Chuck Norris", "Chuck Norris Jokes"],
    "license": "GPL-3.0+",
    "require": {
        "php": "~7.0",
        "aws/aws-sdk-php": "^3.48",
        "bramdevries/oauth2-slack": "~0.1",
        "bramus/monolog-colored-line-formatter": "^2.0",
        "doctrine/dbal": "~2.5",
        "jms/serializer": "~1.2",
        "monolog/monolog": "~1",
        "nochso/html-compress-twig": "~1",
        "silex/silex": "~1.1",
        "symfony/config": "~3.0",
        "symfony/yaml": "~3.0",
        "twig/twig": "~1.2",
        "vlucas/phpdotenv": "^2.3",
        "predis/predis": "^1.1",
        "ramsey/uuid": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "Chuck\\": "src/Lib",
            "Chuck\\App\\": "src"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "scripts": {
        "compile": "bin/populate_cache",
        "serve": "bin/start.sh"
    }
}
